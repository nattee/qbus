= simple_form_for(@licensee) do |f|
  = f.error_notification
  .row
    .col.s12
      .row
        .input-field.col.s12
          %i.material-icons.prefix face
          = f.input_field :name
          = f.label :name
          = f.full_error :name
      .row
        .input-field.col.s12
          %i.material-icons.prefix assignment
          = f.input_field :license_no
          = f.label :license_no
          = f.full_error :license_no
      .row
        .input-field.col.s12
          %i.material-icons.prefix local_taxi
          = f.input_field :car_count
          = f.label :car_count
          = f.full_error :car_count
      .row
        .input-field.col.s12
          %i.material-icons.prefix contacts
          = f.input_field :contact
          = f.label :contact
          = f.full_error :contact
      .row
        .input-field.col.s12
          %i.material-icons.prefix contact_phone
          = f.input_field :contact_tel
          = f.label :contact_tel
          = f.full_error :contact_tel
      .row
        .input-field.col.s12
          %i.material-icons.prefix email
          = f.input_field :contact_email
          = f.label :contact_email
          = f.full_error :contact_email
  = f.button :submit, class: "btn waves-light left"
- if back_link
  = link_to t(:cancel), back_link, style: "margin-left: 2em", class: "btn white black-text waves-black waves-effect"
